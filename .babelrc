	"presets": [
		["@babel/preset-env", {"useBuiltIns": "entry"}],
		"@babel/preset-react"
	],
	"plugins": [
    ["@babel/plugin-transform-runtime"],
		["import", { "libraryName": "antd", "libraryDirectory": "lib", "style": true}, "antd"],
		["import", { "libraryName": "antd-mobile", "libraryDirectory": "lib", "style": true}, "antd-mobile"]
	]
}
